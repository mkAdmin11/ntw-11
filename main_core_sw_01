
en
conf t

service password-encryption

aaa new-model

username admin secret admin
enable secret passwd

aaa authentication login default local 
aaa authentication enable default enable

ip ssh version 2
ip domain-name my.local

crypto key generate rsa
1024

ip access-list extended SSH_ACL
permit tcp 10.1.255.0 0.0.0.127 any
deny ip any any
exit

line con 0
login authentication default
privilege level 15
exit

line aux 0
login authentication default
privilege level 15
exit

line vty 0 4
access-class SSH_ACL in
login authentication default
transport input ssh
privilege level 15
exit

spanning-tree mode rapid-pvst
spanning-tree vlan 1,100,200,300,400,500,1002-1005 root primary

vlan 100
name MGMT
exit

vlan 200
name USER
exit

vlan 300
name WLAN
exit

vlan 400
name VOIP
exit

vlan 500
name PRINTER
exit

interface Vlan100
description MGMT
ip access-group MGMT_ACL out
ip address 10.1.255.1 255.255.255.128
standby version 2
standby 1 ip 10.1.255.3
standby 1 priority 101
standby 1 preempt
standby 0 preempt
exit

interface Vlan200
description USERS
ip access-group USERS_ACL out
ip address 10.1.2.1 255.255.255.0
ip helper-address 10.1.255.126
standby version 2
standby 2 ip 10.1.2.3
standby 2 priority 101
standby 2 preempt
standby 0 preempt
exit

interface Vlan300
description WLAN
ip access-group WLAN_ACL out
ip address 10.1.3.1 255.255.255.0
ip helper-address 10.1.255.126
standby version 2
standby 3 ip 10.1.3.3
standby 3 priority 101
standby 3 preempt
standby 0 preempt
exit

interface Vlan400
description VOIP
ip access-group VOIP_ACL out
ip address 10.1.4.1 255.255.255.0
ip helper-address 10.1.255.126
standby version 2
standby 4 ip 10.1.4.3
standby 4 priority 101
standby 4 preempt
standby 0 preempt
exit

interface Vlan500
description PRINT
ip access-group PRINT_ACL out
ip address 10.1.5.1 255.255.255.0
ip helper-address 10.1.255.126
standby version 2
standby 5 ip 10.1.5.3
standby 5 priority 101
standby 5 preempt
standby 0 preempt
exit

interface range fastEthernet 0/1-3
description MAIN_ACCESS_LAYER
switchport trunk encapsulation dot1q
switchport mode trunk
switchport trunk allowed vlan 100,200,300,400,500
no shutdown
exit

interface range fastEthernet 0/4-22
shutdown
exit

interface range fastEthernet 0/23-24
description PORT-CHANNEL1
channel-group 1 mode active
no shutdown
exit

interface Port-channel1
description RESERVE_MAIN_CORE_SW_02
switchport trunk encapsulation dot1q
switchport mode trunk
switchport trunk allowed vlan 100,200,300,400,500
exit

interface GigabitEthernet0/1
description UPLINK_MAIN_ASA_01
no switchport
ip address 10.0.1.6 255.255.255.252
no shutdown
exit

interface GigabitEthernet0/2
shutdown
exit

ip routing

ip access-list extended MGMT_ACL
remark PERMIT_ANY
101 permit ip any any
!remark DENY_ANY
!901 deny ip any any
exit

ip access-list extended USERS_ACL
remark PERMIT_ANY
101 permit ip any any
!remark DENY_ANY
!901 deny ip any any
exit

ip access-list extended WLAN_ACL
remark PERMIT_ANY
101 permit ip any any
!remark DENY_ANY
!901 deny ip any any
exit

ip access-list extended VOIP_ACL
remark PERMIT_ANY
101 permit ip any any
!remark DENY_ANY
!901 deny ip any any
exit

ip access-list extended PRINT_ACL
remark PERMIT_ANY
101 permit ip any any
!remark DENY_ANY
!901 deny ip any any
exit

exit

wr

