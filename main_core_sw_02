
en
conf t

clock timezone MSK 3
ntp server 10.1.255.126
!ntp update-calendar

no ip domain-lookup

service password-encryption

aaa new-model

username admin privilege 15 secret passwd
enable secret level 15 5 passwd
tacacs-server host 10.1.255.126 key tacasc_server_pass

aaa authentication login default group tacacs+ local
aaa authentication enable default enable

ip ssh version 2
ip ssh time-out 60
ip ssh authentication-retries 3

ip domain-name my.local
crypto key generate rsa
1024

spanning-tree mode rapid-pvst
spanning-tree vlan 1,100,200,300,400,500,1002-1005 priority 40960

ip dhcp snooping
ip dhcp snooping vlan 1,100,200,300,400,500,1002-1005
ip dhcp snooping information option allow-untrusted
!ip dhcp relay information trust-all
!no ip dhcp snooping information option

ip access-list extended SSH
permit tcp 10.1.255.0 0.0.0.127 any
deny ip any any
exit

line con 0
login authentication default
privilege level 15
exit

line vty 0 4
access-class SSH in
login authentication default
transport input ssh
privilege level 15
exit

line vty 5 15
access-class SSH in
login authentication default
transport input ssh
privilege level 15
exit

vlan 100
name MGMT
exit

vlan 200
name USERS
exit

vlan 300
name WLAN
exit

vlan 400
name VOIP
exit

vlan 500
name PRINT
exit

interface Vlan100
description MGMT
ip access-group MGMT_ACL out
ip address 10.1.255.2 255.255.255.128
standby version 2
standby 1 ip 10.1.255.3
standby 1 priority 101
standby 1 preempt
standby preempt
exit

interface Vlan200
description USERS
ip access-group USERS_ACL out
ip address 10.1.2.2 255.255.255.0
ip helper-address 10.1.255.126
standby version 2
standby 2 ip 10.1.2.3
standby 2 priority 101
standby 2 preempt
standby preempt
exit

interface Vlan300
description WLAN
ip access-group WLAN_ACL out
ip address 10.1.3.2 255.255.255.0
ip helper-address 10.1.255.126
standby version 2
standby 3 ip 10.1.3.3
standby 3 priority 101
standby 3 preempt
standby preempt
exit

interface Vlan400
description VOIP
ip access-group VOIP_ACL out
ip address 10.1.4.2 255.255.255.0
ip helper-address 10.1.255.126
standby version 2
standby 4 ip 10.1.4.3
standby 4 priority 101
standby 4 preempt
standby preempt
exit

interface Vlan500
description PRINT
ip access-group PRINT_ACL out
ip address 10.1.5.2 255.255.255.0
ip helper-address 10.1.255.126
standby version 2
standby 5 ip 10.1.5.3
standby 5 priority 101
standby 5 preempt
standby preempt
exit

interface range fastEthernet 0/1-2
description MAIN_ACCESS_LAYER
switchport trunk encapsulation dot1q
switchport mode trunk
switchport trunk allowed vlan 100,200,300,400,500
no shutdown
exit

interface fastEthernet 0/3
description MAIN_ACCESS_LAYER_&_INFRASTRUCTURE
switchport trunk encapsulation dot1q
switchport mode trunk
switchport trunk allowed vlan 100,200,300,400,500
ip dhcp snooping trust
no shutdown
exit

interface range fastEthernet 0/4-22
shutdown
exit

interface range fastEthernet 0/23-24
description PORT-CHANNEL1
channel-group 1 mode passive
ip dhcp snooping trust
no shutdown
exit

interface Port-channel1
description RESERVE_MAIN_CORE_SW_01
switchport trunk encapsulation dot1q
switchport mode trunk
switchport trunk allowed vlan 100,200,300,400,500
exit

interface GigabitEthernet0/1
description UPLINK_MAIN_ASA_02
no switchport
ip address 10.0.2.6 255.255.255.252
no shutdown
exit

interface GigabitEthernet0/2
shutdown
exit

ip routing

ip access-list extended MGMT_ACL
remark PERMIT_ANY
101 permit ip any any
!remark DENY_ANY
!901 deny ip any any
exit

ip access-list extended USERS_ACL
remark PERMIT_ANY
101 permit ip any any
!remark DENY_ANY
!901 deny ip any any
exit

ip access-list extended WLAN_ACL
remark PERMIT_ANY
101 permit ip any any
!remark DENY_ANY
!901 deny ip any any
exit

ip access-list extended VOIP_ACL
remark PERMIT_ANY
101 permit ip any any
!remark DENY_ANY
!901 deny ip any any
exit

ip access-list extended PRINT_ACL
remark PERMIT_ANY
101 permit ip any any
!remark DENY_ANY
!901 deny ip any any
exit

exit

wr

exit
