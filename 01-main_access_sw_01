
en
conf t

hostname main_access_sw_01

clock timezone MSK 3
ntp server 172.17.1.126
ntp trusted-key 4294967282

ip name-server 172.17.1.126
! no ip domain-lookup

service password-encryption

aaa new-model

username admin privilege 15 secret passwd
enable secret passwd

tacacs-server host 172.17.1.126 key tacasc_server_pass

aaa authentication login default group tacacs+ local
aaa authentication enable default group tacacs+ local enable

ip ssh version 2
ip ssh time-out 60
ip ssh authentication-retries 3

ip domain-name my.local
crypto key generate rsa
1024

ip access-list extended SSH_ACL
remark ADMIN_IN_MGMT_NETWORK
1 permit tcp 172.17.1.0 0.0.0.127 any
remark DENY_ANY
10 deny ip any any
exit

line con 0
login authentication default
privilege level 15
exit

line vty 0 4
access-class SSH_ACL in
login authentication default
transport input ssh
privilege level 15
exit

line vty 5 15
access-class SSH_ACL in
login authentication default
transport input ssh
privilege level 15
exit

vlan 100
name MGMT
exit

vlan 200
name USERS
exit

vlan 300
name WLAN
exit

vlan 400
name VOIP
exit

vlan 500
name PRINT
exit

spanning-tree mode rapid-pvst
spanning-tree vlan 1,100,200,300,400,500,1002-1005 priority 40960

ip dhcp snooping
ip dhcp snooping vlan 1,100,200,300,400,500,1002-1005
ip dhcp snooping information option allow-untrusted

interface Vlan100
description MGMT
ip address 172.17.1.11 255.255.255.128
exit

ip default-gateway 172.17.1.1

interface GigabitEthernet0/1
description UPLINK_MAIN_CORE_SW_01
switchport mode trunk
switchport trunk allowed vlan 100,200,300,400,500
ip dhcp snooping trust
exit

interface GigabitEthernet0/2
description RESERV_UPLINK_MAIN_CORE_SW_02
switchport mode trunk
switchport trunk allowed vlan 100,200,300,400,500
spanning-tree vlan 1,100,200,300,400,500,1002-1005 cost 25
ip dhcp snooping trust
exit

interface FastEthernet0/1
description USERS_&_PHONE
switchport mode access
switchport access vlan 200
switchport voice vlan 400
switchport port-security
switchport port-security mac-address sticky 
switchport port-security maximum 4
switchport port-security violation restrict
switchport port-security aging time 1440
spanning-tree portfast
spanning-tree bpduguard enable
exit

interface FastEthernet0/2
description WLC_&_WIFI_USERS
switchport mode trunk
switchport trunk allowed vlan 100,300
switchport trunk native vlan 100
spanning-tree portfast trunk
spanning-tree bpduguard enable
exit

interface range FastEthernet0/3-24
shutdown
exit

exit

wr

exit
